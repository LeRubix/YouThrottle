{
    "manifest_version": 3,
    "name": "YouThrottle",
    "version": "1.0",
    "description": "Removes code from a specific JavaScript file",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  